<%= link_to "Back to all dishes", dishes_path %>
<%= render "/shared_views/errors" %>
<%= image_tag @dish.image_url(:middle).to_s %>
<%= form_for @dish do |form| %>
	<%= render form %>
	<div>
  	<%= form.submit "Update" %> 
  	<%= link_to "delete", dish_path, method: :delete, :confirm => 'Are you sure?' %>
	</div>
<% end %>