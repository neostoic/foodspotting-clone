<%= link_to "Back to all Dishes", dishes_path %>
<%= image_tag @dish.image_url(:middle).to_s %>
<h2><%= @dish.title %></h2>
<h2><%= @dish.description %></h2>
<h3><%= @dish.city %></h3>
<h4>| <%=@dish.user_id %> | <h4>
<%= link_to "Edit Dish", edit_dish_path(@dish) %>
<hr>
<% @dish.likes.pluck(:user_id).each do |like| %>
	<%= like %>
<% end %>